{
  "name": "hackages",
  "version": "0.0.10",
  "description": "cli tools to build fast and scalable applications",
  "main": "tools/parser/index.js",
  "scripts": {
    "preci": "rm -rf mocha.json && npm i",
    "ci": "mocha -R mocha-bamboo-reporter",
    "test": "mocha test/*.ts",
    "test:watch": "npm test -- --watch",
    "codecov": "mocha -- -R -spec && codecov",
    "precommit": "npm run lint",
    "build": "webpack",
    "build:watch": "webpack -w",
    "start": "npm-run-all --parallel test:watch build:watch",
    "prestart": "npm i && npm run typings",
    "typings": "typings install"
  },
  "bin": {
    "hackages": "bin/index.js",
    "hack": "bin/index.js"
  },
  "keywords": [
    "angular",
    "react",
    "ampersand",
    "hackages",
    "javascript"
  ],
  "author": "team@hackages.io",
  "homepage": "https://github.com/hackages/hackages-cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hackages/hackages-cli.git"
  },
